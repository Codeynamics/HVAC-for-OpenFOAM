// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Thermal comfort parameters according to ISO 7730
// 1 met = 58.15 W/mÂ²

clo     0.3;    // Clothing insulation [clo]
met     1.2;    // Metabolic rate [met]
wme     0;      // External work [met] (usually 0)
RH      50;     // Default relative humidity [%] (used if no humidity field available)

