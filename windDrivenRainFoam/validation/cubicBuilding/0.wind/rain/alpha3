FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha3;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            windDrivenRainInlet;
        value           uniform 0;
    }
    
    outlet
    {
        type            zeroGradient;
    }
    
    ground
    {
        type            zeroGradient;
    }
    
    building
    {
        type            catchRatio;
        value           uniform 0;
    }
    
    top
    {
        type            zeroGradient;
    }
    
    sides
    {
        type            symmetryPlane;
    }
}

