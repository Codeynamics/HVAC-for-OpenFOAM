FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            windDrivenRainInlet;
        value           uniform 0;
    }
    
    outlet
    {
        type            zeroGradient;
    }
    
    ground
    {
        type            zeroGradient;
    }
    
    top
    {
        type            zeroGradient;
    }
    
    sideY1
    {
        type            symmetryPlane;
    }
    
    sideY2
    {
        type            symmetryPlane;
    }
    
    oldInternalFaces
    {
        type            catchRatio;
        value           uniform 0;
    }
}

