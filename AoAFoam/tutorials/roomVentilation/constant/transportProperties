// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel  Newtonian;

// Laminar diffusivity for Age of Air [m2/s]
// Typical value for air: 2.5e-5 m2/s
DT              DT [ 0 2 -1 0 0 0 0 ] 2.5e-5;

// Kinematic viscosity for turbulence model [m2/s]
nu              nu [ 0 2 -1 0 0 0 0 ] 1.5e-5;

